{"ast":null,"code":"import axios from \"axios\";\nimport { computed, reactive } from \"vue\";\nimport lib from \"@/scripts/lib\";\nexport default {\n  name: 'Orders',\n  setup() {\n    const state = reactive({\n      orders: []\n    });\n    const load = () => axios.get('/api/cart/items').then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const computedPrice = computed(() => {\n      let result = 0;\n      for (let i of state.items) {\n        result += i.price - i.price * i.discountPer / 100;\n      }\n      return result;\n    });\n    const submit = () => {\n      const args = JSON.parse(JSON.stringify(state.form)); // 참조값 연결고리 끊기 위해\n      args.items = JSON.stringify(state.items);\n      axios.post('/api/orders', args).then(() => {\n        console.log('성공');\n      });\n    };\n    load();\n    return {\n      state,\n      lib,\n      computedPrice,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["axios","computed","reactive","lib","name","setup","state","orders","load","get","then","data","items","computedPrice","result","i","price","discountPer","submit","args","JSON","parse","stringify","form","post","console","log"],"sources":["C:\\dev-projects\\business\\vue-ex\\shoppingmall\\front-end\\src\\pages\\Orders.vue"],"sourcesContent":["<template>\r\n  <div class=\"orders\">\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport {computed, reactive} from \"vue\";\r\nimport lib from \"@/scripts/lib\";\r\n\r\nexport default {\r\n  name: 'Orders',\r\n  setup() {\r\n    const state = reactive({\r\n      orders: [],\r\n    })\r\n\r\n    const load = () => axios.get('/api/cart/items').then(({data}) => {\r\n      state.items = data;\r\n    })\r\n\r\n    const computedPrice = computed(() => {\r\n      let result = 0;\r\n      for(let i of state.items) {\r\n        result += i.price - i.price * i.discountPer / 100;\r\n      }\r\n      return result;\r\n    })\r\n\r\n    const submit = () => {\r\n      const args = JSON.parse(JSON.stringify(state.form)); // 참조값 연결고리 끊기 위해\r\n      args.items = JSON.stringify(state.items);\r\n\r\n      axios.post('/api/orders', args).then(() => {\r\n        console.log('성공')\r\n      })\r\n    }\r\n\r\n    load();\r\n\r\n    return {state, lib, computedPrice, submit}\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n</style>"],"mappings":"AAQA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,EAAEC,QAAQ,QAAO,KAAK;AACtC,OAAOC,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;MACrBK,MAAM,EAAE;IACV,CAAC;IAED,MAAMC,IAAG,GAAIA,CAAA,KAAMR,KAAK,CAACS,GAAG,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MAC/DL,KAAK,CAACM,KAAI,GAAID,IAAI;IACpB,CAAC;IAED,MAAME,aAAY,GAAIZ,QAAQ,CAAC,MAAM;MACnC,IAAIa,MAAK,GAAI,CAAC;MACd,KAAI,IAAIC,CAAA,IAAKT,KAAK,CAACM,KAAK,EAAE;QACxBE,MAAK,IAAKC,CAAC,CAACC,KAAI,GAAID,CAAC,CAACC,KAAI,GAAID,CAAC,CAACE,WAAU,GAAI,GAAG;MACnD;MACA,OAAOH,MAAM;IACf,CAAC;IAED,MAAMI,MAAK,GAAIA,CAAA,KAAM;MACnB,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAAChB,KAAK,CAACiB,IAAI,CAAC,CAAC,EAAE;MACrDJ,IAAI,CAACP,KAAI,GAAIQ,IAAI,CAACE,SAAS,CAAChB,KAAK,CAACM,KAAK,CAAC;MAExCZ,KAAK,CAACwB,IAAI,CAAC,aAAa,EAAEL,IAAI,CAAC,CAACT,IAAI,CAAC,MAAM;QACzCe,OAAO,CAACC,GAAG,CAAC,IAAI;MAClB,CAAC;IACH;IAEAlB,IAAI,CAAC,CAAC;IAEN,OAAO;MAACF,KAAK;MAAEH,GAAG;MAAEU,aAAa;MAAEK;IAAM;EAC3C;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
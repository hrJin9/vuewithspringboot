{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive } from \"vue\";\nimport axios from \"axios\";\nimport store from \"@/scripts/store\";\nimport router from \"@/scripts/router\";\nexport default {\n  name: 'Login',\n  setup() {\n    const state = reactive({\n      form: {\n        email: '',\n        password: ''\n      }\n    });\n    const submit = () => {\n      axios.post(\"/api/account/login\", state.form).then(res => {\n        // store에 로그인 정보 저장\n        store.commit('setAccount', res.data);\n        // 새로고침시 로그인이 풀림 => session에 저장\n        sessionStorage.setItem('id', res.data);\n        router.push({\n          path: '/'\n        });\n        alert('로그인하였습니다.');\n      }).catch(() => {\n        alert('로그인 정보가 존재하지 않습니다.');\n      });\n    };\n    return {\n      state,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","store","router","name","setup","state","form","email","password","submit","post","then","res","commit","data","sessionStorage","setItem","push","path","alert","catch"],"sources":["C:\\dev-projects\\business\\vue-ex\\shoppingmall\\front-end\\src\\pages\\Cart.vue"],"sourcesContent":["<template>\r\n  <div>CartPage</div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive} from \"vue\";\r\nimport axios from \"axios\";\r\nimport store from \"@/scripts/store\";\r\nimport router from \"@/scripts/router\";\r\n\r\nexport default {\r\n  name: 'Login',\r\n  setup() {\r\n    const state = reactive({\r\n      form: {\r\n        email: '',\r\n        password: ''\r\n      }\r\n    })\r\n\r\n    const submit = () => {\r\n      axios.post(\"/api/account/login\", state.form).then((res) => {\r\n        // store에 로그인 정보 저장\r\n        store.commit('setAccount', res.data);\r\n        // 새로고침시 로그인이 풀림 => session에 저장\r\n        sessionStorage.setItem('id', res.data);\r\n        router.push({\r\n          path: '/'\r\n        })\r\n        alert('로그인하였습니다.');\r\n      }).catch(() => {\r\n        alert('로그인 정보가 존재하지 않습니다.')\r\n      })\r\n    }\r\n\r\n\r\n    return {state, submit};\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.form-signin {\r\n  max-width: 330px;\r\n  padding: 1rem;\r\n}\r\n\r\n.form-signin .form-floating:focus-within {\r\n  z-index: 2;\r\n}\r\n\r\n.form-signin input[type=\"email\"] {\r\n  margin-bottom: -1px;\r\n  border-bottom-right-radius: 0;\r\n  border-bottom-left-radius: 0;\r\n}\r\n\r\n.form-signin input[type=\"password\"] {\r\n  margin-bottom: 10px;\r\n  border-top-left-radius: 0;\r\n  border-top-right-radius: 0;\r\n}\r\n</style>"],"mappings":";AAKA,SAAQA,QAAQ,QAAO,KAAK;AAC5B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,iBAAiB;AACnC,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF,CAAC;IAED,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBT,KAAK,CAACU,IAAI,CAAC,oBAAoB,EAAEL,KAAK,CAACC,IAAI,CAAC,CAACK,IAAI,CAAEC,GAAG,IAAK;QACzD;QACAX,KAAK,CAACY,MAAM,CAAC,YAAY,EAAED,GAAG,CAACE,IAAI,CAAC;QACpC;QACAC,cAAc,CAACC,OAAO,CAAC,IAAI,EAAEJ,GAAG,CAACE,IAAI,CAAC;QACtCZ,MAAM,CAACe,IAAI,CAAC;UACVC,IAAI,EAAE;QACR,CAAC;QACDC,KAAK,CAAC,WAAW,CAAC;MACpB,CAAC,CAAC,CAACC,KAAK,CAAC,MAAM;QACbD,KAAK,CAAC,oBAAoB;MAC5B,CAAC;IACH;IAGA,OAAO;MAACd,KAAK;MAAEI;IAAM,CAAC;EACxB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import axios from \"axios\";\nimport { computed, reactive } from \"vue\";\nimport lib from \"@/scripts/lib\";\nexport default {\n  name: 'Orders',\n  setup() {\n    const state = reactive({\n      items: [],\n      form: {\n        name: '',\n        address: '',\n        payment: '',\n        cardNumber: '',\n        items: ''\n      }\n    });\n    const load = () => axios.get('/api/cart/items').then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const computedPrice = computed(() => {\n      let result = 0;\n      for (let i of state.items) {\n        result += i.price - i.price * i.discountPer / 100;\n      }\n      return result;\n    });\n    const submit = () => {\n      const args = JSON.parse(JSON.stringify(state.form)); // 참조값 연결고리 끊기 위해\n      args.items = JSON.stringify(state.items);\n      axios.post('/api/orders', args).then(() => {\n        console.log('성공');\n      });\n    };\n    load();\n    return {\n      state,\n      lib,\n      computedPrice,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["axios","computed","reactive","lib","name","setup","state","items","form","address","payment","cardNumber","load","get","then","data","computedPrice","result","i","price","discountPer","submit","args","JSON","parse","stringify","post","console","log"],"sources":["C:\\dev-projects\\business\\vue-ex\\shoppingmall\\front-end\\src\\pages\\Orders.vue"],"sourcesContent":["<template>\r\n  <div class=\"orders\">\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport {computed, reactive} from \"vue\";\r\nimport lib from \"@/scripts/lib\";\r\n\r\nexport default {\r\n  name: 'Orders',\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n      form: {\r\n        name: '',\r\n        address: '',\r\n        payment: '',\r\n        cardNumber: '',\r\n        items: ''\r\n      }\r\n    })\r\n\r\n    const load = () => axios.get('/api/cart/items').then(({data}) => {\r\n      state.items = data;\r\n    })\r\n\r\n    const computedPrice = computed(() => {\r\n      let result = 0;\r\n      for(let i of state.items) {\r\n        result += i.price - i.price * i.discountPer / 100;\r\n      }\r\n      return result;\r\n    })\r\n\r\n    const submit = () => {\r\n      const args = JSON.parse(JSON.stringify(state.form)); // 참조값 연결고리 끊기 위해\r\n      args.items = JSON.stringify(state.items);\r\n\r\n      axios.post('/api/orders', args).then(() => {\r\n        console.log('성공')\r\n      })\r\n    }\r\n\r\n    load();\r\n\r\n    return {state, lib, computedPrice, submit}\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n</style>"],"mappings":"AAQA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,EAAEC,QAAQ,QAAO,KAAK;AACtC,OAAOC,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;MACrBK,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;QACJJ,IAAI,EAAE,EAAE;QACRK,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE,EAAE;QACdJ,KAAK,EAAE;MACT;IACF,CAAC;IAED,MAAMK,IAAG,GAAIA,CAAA,KAAMZ,KAAK,CAACa,GAAG,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAC,CAAC;MAACC;IAAI,CAAC,KAAK;MAC/DT,KAAK,CAACC,KAAI,GAAIQ,IAAI;IACpB,CAAC;IAED,MAAMC,aAAY,GAAIf,QAAQ,CAAC,MAAM;MACnC,IAAIgB,MAAK,GAAI,CAAC;MACd,KAAI,IAAIC,CAAA,IAAKZ,KAAK,CAACC,KAAK,EAAE;QACxBU,MAAK,IAAKC,CAAC,CAACC,KAAI,GAAID,CAAC,CAACC,KAAI,GAAID,CAAC,CAACE,WAAU,GAAI,GAAG;MACnD;MACA,OAAOH,MAAM;IACf,CAAC;IAED,MAAMI,MAAK,GAAIA,CAAA,KAAM;MACnB,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAACnB,KAAK,CAACE,IAAI,CAAC,CAAC,EAAE;MACrDc,IAAI,CAACf,KAAI,GAAIgB,IAAI,CAACE,SAAS,CAACnB,KAAK,CAACC,KAAK,CAAC;MAExCP,KAAK,CAAC0B,IAAI,CAAC,aAAa,EAAEJ,IAAI,CAAC,CAACR,IAAI,CAAC,MAAM;QACzCa,OAAO,CAACC,GAAG,CAAC,IAAI;MAClB,CAAC;IACH;IAEAhB,IAAI,CAAC,CAAC;IAEN,OAAO;MAACN,KAAK;MAAEH,GAAG;MAAEa,aAAa;MAAEK;IAAM;EAC3C;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}